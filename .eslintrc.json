{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-useless-escape": "off",
        "complexity": [
            "warn",
            10
        ],
        "sort-vars": "warn"
    }
}